{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build && npm run build:copy-assets",
    "build:copy-assets": "mkdir -p .output/public && cp -r .nuxt/dist/client/* .output/public/ && cp -r public/* .output/public/ 2>/dev/null || true",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "slicemachine": "start-slicemachine"
  },
  "devDependencies": {
    "@nuxtjs/prismic": "^3.0.1",
    "@slicemachine/adapter-nuxt": "^0.3.10",
    "@types/node": "^18",
    "nuxt": "^3.5.2",
    "slice-machine-ui": "^1.8.0"
  },
  "dependencies": {
    "vue-silentbox": "^3.0.4"
  }
}
